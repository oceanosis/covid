#!/usr/bin/env bash

# Environment vars

PYTHON_VERSION="3.7.7"
POSTGRES_VERSION="12.2"
CRDB_VERSION="v20.1.0"
APP_VERSION="1.0"

PY_BUILD=covidapp/python:$PYTHON_VERSION-$APP_VERSION
PSQL_BUILD=covidapp/postgres:$POSTGRES_VERSION-$APP_VERSION
CRDB_BUILD=covidapp/cockroach:$CRDB_VERSION-$APP_VERSION

DB_CONTAINER_NAME="devdb"
PY_CONTAINER_NAME="covidapp"

RAWDATA_REPO_URL="https://github.com/CSSEGISandData/COVID-19.git"
